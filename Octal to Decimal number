#include<stdio.h>
#include<math.h>
#include<conio.h>
int main()
{
 int octal;
 int sum=0;
 int a,c;
 int n=0;
 printf("enter a octal number");
 scanf("%d",&octal);
 clrscr();
 while (octal!=0)
 {
  a = octal%10;
  c = pow(8,n);
  sum=sum+(a*c);
  n=n+1;
  octal=octal/10;
 }
 printf("The answer is %d",sum);
}
